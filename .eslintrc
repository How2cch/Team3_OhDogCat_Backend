{
  "env": {
    "browser": true,
    "es2022": true,
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": ["react-app"],
  "plugins": ["prettier", "react-hooks"],
  "rules": {
    "prettier/prettier": "error",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
}
